{
    "name" : "Force SafeSearch",
    "manifest_version": 2,
    "version" : "0.2.1",
    "permissions" : ["https://*.google.com/*", "https://*.youtube.com/*", "http://*.bing.com/*"],
    "description" : "Enables SafeSearch on Google, YouTube and Bing.",
    "background": {
        "scripts": ["jquery-1.11.1.min.js", "forceSafeSearch.js"],
        "persistent": false
    },
    "icons": { "16": "icon16.png",
        "22": "icon22.png",
        "48": "icon48.png",
        "64": "icon64.png",
        "128": "icon128.png" }
}